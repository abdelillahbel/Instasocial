<resources>

    <declare-styleable name="MyTheme">
        <attr name="actionbarCompatTitleStyle" format="reference" />
        <attr name="actionbarCompatItemStyle" format="reference" />
        <attr name="actionbarCompatItemHomeStyle" format="reference" />
        <attr name="actionbarCompatProgressIndicatorStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="BezelImageView">
        <attr name="maskDrawable" format="reference" />
        <attr name="borderDrawable" format="reference" />
    </declare-styleable>



    <declare-styleable name="CountryCodePicker">
        <attr name="defaultCode" format="integer" />
        <attr name="contentColor" format="color"/>
        <attr name="arrowSize" format="dimension"/>
        <attr name="countryPreference" format="string"/>
        <attr name="defaultNameCode" format="string" />
        <attr name="hideNameCode" format="boolean"/>
        <attr name="textSizeOne" format="dimension"/>
        <attr name="ccpLanguage" format="enum">
            <enum name="ARABIC" value="1"/>
            <enum name="BENGALI" value="2"/>
            <enum name="CHINESE" value="3"/>
            <enum name="ENGLISH" value="4"/>
            <enum name="FRENCH" value="5"/>
            <enum name="GERMAN" value="6"/>
            <enum name="GUJARATI" value="7"/>
            <enum name="HINDI" value="8"/>
            <enum name="JAPANESE" value="9"/>
            <enum name="JAVANESE" value="10"/>
            <enum name="PORTUGUESE" value="11"/>
            <enum name="RUSSIAN" value="12"/>
            <enum name="SPANISH" value="13"/>
        </attr>
        <attr name="keyboardAutoPopOnSearch" format="boolean"/>
        <attr name="customMasterCountries" format="string" />
        <attr name="showFlag" format="boolean" />
        <attr name="showFullName" format="boolean" />
    </declare-styleable>


    <attr name="fontPath" format="string|reference" />

    <declare-styleable name="CompatTextView">
        <attr name="fontPath" />
    </declare-styleable>

    <declare-styleable name="CompatEditText">
        <attr name="fontPath" />
    </declare-styleable>

    <declare-styleable name="CompatButton">
        <attr name="fontPath" />
    </declare-styleable>

    <declare-styleable name="CompatImageView">
        <attr name="filterColor" format="color" />
    </declare-styleable>
</resources>
